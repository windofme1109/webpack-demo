{
  "presets": [
    [
      "@babel/preset-env", {

        "targets": {
          "firefox": "60",
          "chrome": "67"
        },

        "useBuiltIns": "usage"
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    [
      "@babel/plugin-transform-runtime", {
      "absoluteRuntime": false,
      "corejs": false,
      "helpers": true,
      "regenerator": true,
      "useESModules": false
    }
    ]
  ]
}